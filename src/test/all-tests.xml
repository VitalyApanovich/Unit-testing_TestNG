<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test NG tests" verbose="2" parallel="tests" group-by-instances="true">
    <test name="Virtual Item Test">
        <classes>
            <class name="VirtualItemTest"/>
        </classes>
    </test>
    <test name="Cart Test" group-by-instances="true">
        <classes>
            <class name="CartTest"/>
        </classes>
    </test>
    <test name="Parser Test" group-by-instances="true">
        <classes>
            <class name="ParserTest">
                <methods>
                    <exclude name="readFromFileExpectsFileClassTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="RealItem Test" group-by-instances="true">
        <classes>
            <class name="RealItemTest"/>
        </classes>
    </test>
</suite>